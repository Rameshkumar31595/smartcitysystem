{
  "version": 2,
  "builds": [
    {
      "src": "smartcityportal/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "smartcityportal/wsgi.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt"
}