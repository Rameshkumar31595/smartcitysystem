{
  "version": 2,
  "builds": [
    {
      "src": "smartcityportal/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "smartcityportal/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "smartcityportal.settings"
    }
  }
}